        <TextBox x:Name="txtBoxHashKod" HorizontalAlignment="Left" Margin="93,42,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="387"/>
        <Label Content="Hash kód:" HorizontalAlignment="Left" Margin="30,38,0,0" VerticalAlignment="Top"/>
        <Label Content="Cég:" HorizontalAlignment="Left" Margin="59,69,0,0" VerticalAlignment="Top" RenderTransformOrigin="1.162,0.503"/>
        <RadioButton x:Name="rbWayteq" Content="WayteQ" HorizontalAlignment="Left" Margin="108,75,0,0" VerticalAlignment="Top" IsChecked="True"/>
        <RadioButton x:Name="rbMiStores" Content="Mi Stores Hungary" HorizontalAlignment="Left" Margin="268,75,0,0" VerticalAlignment="Top"/>
        <Label x:Name="lblEleresiUt" Content="Elérési Út:" HorizontalAlignment="Left" Margin="93,95,0,0" VerticalAlignment="Top"/>
        <Label x:Name="lblTest" Content="Label" HorizontalAlignment="Left" Margin="99,115,0,0" VerticalAlignment="Top"/>

    </Grid>
</Window>

        <Button x:Name="btnOpenXLS" Content="XLS megnyitása" HorizontalAlignment="Left" VerticalAlignment="Top" Height="43" Width="114" Cursor="Hand" Background="#FF909FF3" Click="btnOpenXLS_Click" Margin="61,141,0,0"/>
